
{
    "name": "fabric-getrange",
    "version": "1.0.0",
    "type": "flogo:activity",
    "title": "Get Range",
    "author": "TIBCO Software Inc.",
    "display": {
        "category": "fabric",
        "visible": true,
        "smallIcon": "ic-range.png",
        "description": "This activity fetches data by range of keys from fabric ledger"
    },
    "ref": "github.com/yxuco/flogo-enterprise-app/fabric/activity/getrange",
    "homepage": "http://github.com/yxuco/flogo-enterprise-app/fabric/activity/getrange",
    "inputs": [
        {
            "name": "startKey",
            "type": "string",
            "required": true
        },
        {
            "name": "endKey",
            "type": "string",
            "required": true
        },
        {
            "name": "pageSize",
            "type": "number",
            "required": true,
            "display": {
                "name": "Result page size",
                "description": "Result page size, default 0 to return all matching record"
            },
            "value": 0
        },
        {
            "name": "start",
            "type": "string"
        },
        {
            "name": "closed",
            "type": "boolean",
            "display": {
                "name": "Close resultset",
                "description": "Close resultset after this page of records",
                "visible": false            },
            "value": true
        },
        {
            "name": "isPrivate",
            "type": "boolean",
            "required": true,
            "display": {
                "name": "Data is private",
                "description": "Get data from private collection"
            },
            "value": false
        },
        {
            "name": "collection",
            "type": "string",
            "display": {
                "name": "Private collection name",
                "description": "Name of the private collection",
                "appPropertySupport": true,
                "visible": false
            }
        }
    ],
    "outputs": [
        {
            "name": "code",
            "type": "integer"
        },
        {
            "name": "message",
            "type": "string"
        },
        {
            "name": "bookmark",
            "type": "string"
        },
        {
            "name": "result",
            "type": "complex_object",
            "display": {
                "name": "Result array definition",
                "description": "Schema definition of the resulting array of key and value, e.g.,\r\n[{\r\n  \"key\": \"string\",\r\n  \"value\": {\"field1\": \"\", \"field2\": 0}\r\n}]",
                "type": "texteditor",
                "syntax": "json"
            }
        }
    ]
}
